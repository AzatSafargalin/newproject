<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="newproject" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>ru.ilb.common.jpa.converters.UUIDConverterBytes</class>
    <class>ru.ilb.newproject.model.User</class>
    <class>ru.ilb.newproject.model.UserGroup</class>
    <class>ru.ilb.newproject.model.BusinessEntity</class>
    <class>ru.ilb.newproject.model.PrivatePerson</class>
    <class>ru.ilb.newproject.model.SelfEmployed</class>
    <class>ru.ilb.newproject.model.CompanyType</class>
    <class>ru.ilb.newproject.model.LoanTreaty</class>
    <class>ru.ilb.newproject.model.Requirement</class>
    <class>ru.ilb.newproject.model.RequirementType</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="eclipselink.logging.level" value="FINE"/>
      <property name="eclipselink.jdbc.batch-writing.size" value="1000"/>
      <property name="eclipselink.jdbc.batch-writing" value="JDBC"/>
      <property name="eclipselink.weaving" value="static"/>
    </properties>
  </persistence-unit>
</persistence>
